{
  "name": "annotation-book",
  "version": "0.0.1",
  "description": "Personal Gitbook to store notes, tutorials and whatever annotation I need",
  "main": "index.js",
  "repository": "git@github.com:raulfdm/annotation-book.git",
  "author": "Raul de Melo",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:book": "gitbook build",
    "build:deploy": "surge -p _book/ -d raulfdm-gitbook.surge.sh",
    "build": "npm-run-all build:book build:deploy",
    "start": "gitbook serve",
    "prepush": "npm run build"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "gitbook-plugin-advanced-emoji": "^0.2.2"
  }
}
